{
  "name": "sourcebin",
  "version": "5.0.0",
  "description": "Fast and simple package to get and create bins from https://sourceb.in/",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@sourcebin/linguist": "^0.0.3",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "nodemon": "^2.0.19",
    "tsm": "^2.2.2",
    "tsup": "^6.2.3",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.2",
    "uvu": "^0.5.6"
  },
  "keywords": [
    "sourcebin",
    "bin",
    "bin wrapper",
    "sourcebin wrapper",
    "source",
    "source bin",
    "sourcebin api",
    "sourcebin api wrapper",
    "sourcebin create",
    "sourcebin get"
  ],
  "author": "GHOST",
  "homepage": "https://github.com/ghostdevv/sourcebin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostdevv/sourcebin"
  },
  "bugs": {
    "url": "https://github.com/ghostdevv/sourcebin"
  },
  "scripts": {
    "test": "tsm node_modules/uvu/bin.js tests \".*\\.test\\.ts\"",
    "dev": "nodemon dev/index.js",
    "docs": "typedoc src/index.ts",
    "build": "tsup-node"
  }
}